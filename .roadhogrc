{
  "entry": "src/index.js",
  "extraBabelPlugins": [
    ["module-resolver", { 
      "alias": {
          "src": "./src"
				}
		}],
    "transform-runtime",
    "transform-decorators-legacy",
    "transform-class-properties",
    ["import", { "libraryName": "antd", "libraryDirectory": "es", "style": true }]
  ],
  "env": {
    "development": {
      "extraBabelPlugins": [
        "dva-hmr"
      ],
      "devtool": "cheap-module-eval-source-map"
    },
    "production": {
      "devtool": "source-map"
    }
  },
  "externals": {
    //"g2": "G2",
    //"g-cloud": "Cloud",
    //"g2-plugin-slider": "G2.Plugin.slider"
  },
  "ignoreMomentLocale": true,
  "theme": "./src/theme.js",
  "hash": true
}
